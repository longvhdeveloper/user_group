<h1>Create Presentation</h1>
<div class="form">
  <%= error_messages_for :presentation %>

  <fieldset>
    <legend>Enter Presentation Details</legend>
    <%= form_for :presentation, :url => {:action => :create, :meeting_id => @meeting.id} do |f| %>
        <%= render :partial => 'form', :locals => {:f => f} %>
        <%= f.submit 'Create', :class => "submit" %> &nbsp;
        <%= link_to 'Cancel', meeting_path(@meeting), :class => 'cancel' %>
    <% end %>
  </fieldset>
</div>